#include <iostream>
using namespace std;

int main() {
    char a = '!';
    cout << int(a);
	return 0;
}

//---------------------------------------------------
#include <bits/stdc++.h> 
using namespace std; 

int main() { 
    string a;
    cin >> a;
  
    int x = a.length(); 
  
    char char_array[x + 1]; 
  
    strcpy(char_array, a.c_str()); 
  
    for (int i = 0; i < x; i++) 
        cout << char_array[i] << " "; 
  
    return 0; 
}
//----------------------------------------------------------
#include <iostream>
int main()
{
  std::cout << "Printable ASCII:\n";
  for (char i = 32; i < 127; ++i) {
    std::cout << i << ' ';
    if (i % 16 == 15)
      std::cout << '\n';
  }
}
