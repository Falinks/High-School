#include <iostream>
using namespace std;

int main() {
    int x;
    cin >> x;
    int arr[x][x];

    for(int i=0; i < x; i++) {
        for(int j=0; j < x; j++) {
            cin >> arr[i][i+j];
        }
    }
    int a;
    for(int i=0; i < x; i++) {
        a += arr[i-1][i-1];
    }
    cout << a;
return 0;
}
